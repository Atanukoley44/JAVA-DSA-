class Solution {
    public int removeDuplicates(int[] nums) {
        int left = 1 ;
        int right = 1 ; 
        int n = nums.length;

        if(nums.length == 0){
            return 0;
        }

        while(right<n){
            if(nums[right] != nums[right-1]){
                nums[left] = nums[right];
                left++;
            }
            right++;
        }
        return left;



        
    }
}
